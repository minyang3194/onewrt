config member 'wan_m1_w1'
	option metric '1'
	option interface 'wan'
	option weight '2'

config member 'wan1_m1_w1'
	option metric '1'
	option interface 'wan1'
	option weight '3'

config member 'wan_m2_w1'
	option metric '2'
	option weight '1'
	option interface 'wan'

config member 'wan1_m2_w1'
	option metric '2'
	option weight '1'
	option interface 'wan1'

config rule 'wan_first'
	option proto 'all'
	option sticky '0'
	option use_policy 'first_wan'
	option ipset 'cnc'

config rule 'wan1_first'
	option proto 'all'
	option sticky '0'
	option use_policy 'first_wan1'
	option ipset 'cmcc'

config rule 'other_bank'
	option dest_port '443,80'
	option proto 'tcp'
	option sticky '0'
	option use_policy 'first_wan'
	option ipset 'cnc'

config rule 'balance'
	option proto 'all'
	option sticky '0'
	option use_policy 'balanced'

config globals 'globals'
	option mmx_mask '0x3F00'
	option local_source 'lan'

config interface 'wan'
	option enabled '1'
	list track_ip '114.114.114.114'
	list track_ip '114.114.115.115'
	option family 'ipv4'
	option reliability '2'
	option count '1'
	option timeout '2'
	option failure_latency '1000'
	option recovery_latency '500'
	option failure_loss '20'
	option recovery_loss '5'
	option interval '5'
	option down '3'
	option up '8'

config policy 'balanced'
	list use_member 'wan_m1_w1'
	list use_member 'wan1_m1_w1'
	option last_resort 'default'

config interface 'wan1'
	option enabled '1'
	option initial_state 'online'
	list track_ip '114.114.114.114'
	list track_ip '114.114.115.115'
	option family 'ipv4'
	option track_method 'ping'
	option reliability '1'
	option count '1'
	option size '56'
	option check_quality '0'
	option timeout '2'
	option interval '5'
	option failure_interval '5'
	option recovery_interval '5'
	option down '3'
	option up '3'
	option flush_conntrack 'never'

config policy 'first_wan'
	list use_member 'wan_m1_w1'
	list use_member 'wan1_m2_w1'
	option last_resort 'default'

config policy 'first_wan1'
	list use_member 'wan1_m1_w1'
	list use_member 'wan_m2_w1'
	option last_resort 'default'
